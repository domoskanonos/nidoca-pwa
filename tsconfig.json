{
  "compilerOptions": {
    "target": "es2015",
    "declaration": true,
    "sourceMap": true,
    "module": "es2015",
    "outDir": "./public/lib/",
    "downlevelIteration": true,
    "strict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  }
}
